{
  "name": "Last URL visited",
  "version": "1",
  "description": "Read the last url visited and send it.",
  "incognito": "split",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["LastUrls.js"],
    "persistent": true
  },
  "manifest_version": 2
}
